<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VeduEnterprises"
        tools:targetApi="31">
        <activity
            android:name=".activityies.activity.CheckoutActivity"
            android:exported="false"
            android:label="@string/title_activity_checkout"
            android:theme="@style/Theme.VeduEnterprises" />
        <activity
            android:name=".activityies.activity.ProfileActivity"
            android:exported="false"
            android:theme="@style/Theme.VeduEnterprises" />
        <activity
            android:name=".activityies.activity.PreviewActivity"
            android:exported="false"
            android:theme="@style/Theme.VeduEnterprises" />
        <activity
            android:name=".activityies.activity.SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.VeduEnterprises">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activityies.activity.MainActivity"
            android:exported="false"
            android:theme="@style/Theme.VeduEnterprises" />
        <activity
            android:name=".activityies.activity.LoginActivity"
            android:exported="true"
            android:theme="@style/Theme.VeduEnterprises" />
    </application>

</manifest>